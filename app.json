{
  "expo": {
    "name": "NadHub",
    "slug": "NadHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "แอพต้องการเข้าถึงรูปภาพของคุณเพื่ออัปโหลดสินค้า",
          "cameraPermission": "แอพต้องการใช้กล้องของคุณเพื่อถ่ายภาพสินค้า"
        }
      ]
    ],
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "626eb644-942f-4d67-9c2d-cdc0774dbce3"
      }
    },
    "android": {
      "package": "com.dotajao.nadhub"
    }
  }
}